
public class seat 
{
	private boolean pay;
	private boolean use;
	private boolean start;
	private int stimehour;
	private int stimemin;
	private String print;
	private String stime;
	private String ftime;
	private int totalprice;
	
	seat(boolean pay,boolean use, boolean start, int stimehour,int stimemin,String print,String stime,String ftime,int totalprice)
	{
		this.pay = pay;
		this.print = print;
		this.use = use;
		this.start = start;
		this.stimehour = stimehour;
		this.stimemin = stimemin;
		this.stime = stime;
		this.stime = ftime;
		this.totalprice = totalprice;
	}
	
	public boolean isUse() {
		return use;
	}

	public void setUse(boolean use) {
		this.use = use;
	}
	
	public boolean ispay() {
		return pay;
	}

	public void setpay(boolean pay) {
		this.pay = pay;
	}

	public boolean isStart() {
		return start;
	}

	public void setStart(boolean start) {
		this.start = start;
	}

	public int getStimehour() {
		return stimehour;
	}

	public void setStimehour(int stimehour) {
		this.stimehour = stimehour;
	}
	
	public int getStimemin() {
		return stimemin;
	}

	public void setStimemin(int stimemin) {
		this.stimemin = stimemin;
	}
	
	public String getprint(){
		return print;
	}

	public void setprint(String print) {
		this.print = print;
	}
	
	public String getstime() {
		return stime;
	}

	public void setstime(String stime) {
		this.stime = stime;
	}
	
	public String getftime() {
		return ftime;
	}

	public void setftime(String ftime) {
		this.ftime = ftime;
	}
	
	public void settotalprice(int totalprice) {
		this.totalprice = totalprice;
	}
	
	public int gettotalprice() {
		return totalprice;
	}
}
